<p id="notice"><%= notice %></p>

<h3>Administrators</h3>

    <% @administrators.each do |administrator| %>
      <li class="media">
          <img src="..." class="align-self-center mr-3" alt="...">
          <div class="media-body">
            <table width="100%"><tr width=100%>
              <td width="90%" id="post">
                <ul class="list-inline">
                  <li class="list-inline-item post-title"></li>
                  <li class="list-inline-item post-info"></li>
                </ul>
                <ul class="list-inline">
                  <li class="list-inline-item post-info"><%= administrator["created_at"] %></li>
                </ul>
              </td>
              <td width="10%" id="votes">
              </td>
            </tr></table>
        </li>
    <% end %>
<br>

<%= link_to 'New Administrator', new_administrator_path %>
